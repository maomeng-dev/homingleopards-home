{
  "name": "homingleopards-home-static",
  "version": "1.0.0",
  "description": "CFCA Homing Leopards Homepage Static Files",
  "main": "index.js",
  "scripts": {
    "start": "node ./scripts/help.js",
    "help": "node ./scripts/help.js",
    "version": "node ./scripts/version.js",
    "dev": "yarn run clean && gulp dev --gulpfile ./config/gulpfile.js --color",
    "prod": "yarn run clean && gulp prod --gulpfile ./config/gulpfile.js --color",
    "server": "gulp server --gulpfile ./config/gulpfile.js --color",
    "clean": "node ./scripts/clean.js",
    "build": "yarn install && yarn run prod",
    "deploy": "yarn run build && yarn run version"
  },
  "author": "Konru",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "chalk": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.9.4",
    "gulp-less": "^4.0.1",
    "gulp-stylus": "^2.7.0",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^5.0.0",
    "rimraf": "^2.6.1",
    "run-sequence": "^2.2.1",
    "webpack": "^3.12.0"
  }
}
